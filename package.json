{
  "name": "skeleton-angular-6",
  "version": "0.1.1",
  "author": "Sergey Romanov",
  "description": "Skeleton Angular 6, Webpack 4, UIRouter, Webpack Dev Server, HMR, AOT, Karma, Jasmine, Protractor, E2E, Docker",
  "main": "src/main.ts",
  "private": true,
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "scripts": {
    "server:dev:hmr": "npm run server:dev -- --hot --hotOnly",
    "server:aot:dev": "cross-env BUILD_AOT=1 npm run server:dev",
    "server:dev": "npm run webpack-dev-server -- --config config/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "server:aot:local": "cross-env BUILD_AOT=1 npm run server:local",
    "server:local": "HOST_CHECK=false NODE_ENV=local npm run webpack-dev-server -- --config config/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "server:local:hmr": "npm run server:local -- --hot --hotOnly",
    "server:prod": "http-server dist -c-1 --cors",
    "server:prod:ci": "http-server dist -p 3000 -c-1 --cors",
    "server": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "start": "npm run server:local",
    "start:dev": "npm run server:dev",
    "start:aot": "npm run server:aot:local",
    "build:local": "npm run webpack -- --config config/webpack.dev.js --progress --profile",
    "build:dev": "npm run clean:dist && npm run webpack -- --config config/webpack.dev.js --progress --profile",
    "build:dev:aot": "cross-env BUILD_AOT=1 npm run build:dev",
    "build:qa": "npm run clean:dist && NODE_ENV=qa npm run webpack -- --config config/webpack.dev.js --progress --profile",
    "build:qa:aot": "cross-env BUILD_AOT=1 npm run build:qa",
    "build:stage": " npm run clean:dist && NODE_ENV=stage npm run webpack -- --config config/webpack.prod.js --progress --profile",
    "build:stage:aot": "cross-env BUILD_AOT=1 npm run build:stage",
    "build:prod": "npm run clean:dist &&  webpack --config config/webpack.prod.js --colors --progress --display-error-details --display-cached --profile",
    "build:prod:aot": "npm run clean:dist && cross-env BUILD_AOT=1  npm run webpack -- --config config/webpack.prod.js  --progress --profile --bail",
    "build:aot": "npm run build:prod:aot",
    "build:docker": "npm run build:prod && docker build -t peoplefirst:latest .",
    "build": "npm run build:dev",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:aot:dev": "npm run build:dev:aot -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:aot:prod": "npm run build:prod:aot -- --watch",
    "watch": "npm run watch:dev",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "node": "node",
    "clean:dist": "rimraf dist",
    "clean:cache": "rimraf webpack-cache",
    "clean:coverage": "rimraf coverage",
    "clean:all": "npm run clean:dist && npm run clean:cache && npm run clean:coverage",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "docs:compodoc": "compodoc -p tsconfig.json",
    "docs:compodoc:serve": "compodoc -p tsconfig.json -s",
    "docs:compodoc:serve:watch": "compodoc -p tsconfig.json -s -w",
    "e2e:live": "npm-run-all -p -r server:prod:ci protractor:live",
    "e2e": "npm-run-all -p -r server:prod:ci protractor",
    "e2e:local": "npm run build:local && npm run e2e",
    "e2e:dev": "npm run build:dev && npm run e2e",
    "e2e:qa": "npm run build:qa && npm run e2e",
    "e2e:stage": "npm run build:stagfe && npm run e2e",
    "e2e:prod": "npm run build:prod && npm run e2e",
    "protractor": "protractor",
    "protractor:delay": "sleep 3 && npm run protractor",
    "protractor:live": "protractor --elementExplorer",
    "test": "karma start",
    "test:sonar": "npm run lint && cross-env SONAR_QUBE=1 karma start",
    "test:single": "karma start --single-run --browsers ChromeTravisCi",
    "tslint": "tslint \"src/**/*.ts\" --project tsconfig.json",
    "typedoc": "typedoc",
    "docker": "docker",
    "webdriver-manager": "node ./node_modules/protractor/bin/webdriver-manager",
    "webdriver:start": "node ./node_modules/protractor/bin/webdriver-manager start",
    "webdriver:update": "node ./node_modules/protractor/bin/webdriver-manager update",
    "postinstall": "npm run webdriver:update"
  },
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/platform-server": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@auth0/angular-jwt": "^2.1.0",
    "@ng-bootstrap/ng-bootstrap": "^3.3.1",
    "@ng-select/ng-select": "^2.14.0",
    "@uirouter/angular": "^2.0.2",
    "@uirouter/rx": "^0.5.0",
    "@uirouter/visualizer": "^6.0.0",
    "bootstrap": "^4.3.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.3",
    "intl": "^1.2.5",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "ng2-validators": "^2.2.0",
    "rxjs": "^6.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.6.8",
    "@angular/compiler-cli": "^6.1.10",
    "@compodoc/compodoc": "^1.1.3",
    "@ngtools/webpack": "^6.1.10",
    "@types/core-js": "^2.5.0",
    "@types/jasmine": "^2.8.9",
    "@types/jquery": "^3.2.15",
    "@types/moment-timezone": "^0.5.6",
    "@types/node": "^8.0.58",
    "@types/webpack": "^3.0.14",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^5.2.0",
    "codelyzer": "^4.3.0",
    "copy-webpack-plugin": "^4.5.1",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "exports-loader": "^0.7.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^2.8.0",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^1.1.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-remap-coverage": "^0.1.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "null-loader": "^0.1.1",
    "optimize-js-plugin": "^0.0.4",
    "postcss-loader": "^2.0.8",
    "protractor": "^5.3.2",
    "puppeteer": "^1.12.2",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.2.1",
    "rimraf": "^2.6.2",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "to-string-loader": "^1.1.5",
    "ts-node": "^7.0.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typescript": "~2.8.0",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-inline-manifest-plugin": "^4.0.1",
    "webpack-merge": "^4.1.0",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
